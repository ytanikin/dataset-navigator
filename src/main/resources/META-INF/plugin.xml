<idea-plugin>
    <id>com.ytanikin.datasetnavigator</id>
    <name>Dataset Navigator</name>
    <vendor>ytanikin</vendor>
    <!-- Product and plugin compatibility requirements -->
    <!-- https://plugins.jetbrains.com/docs/intellij/plugin-compatibility.html -->
    <depends>com.intellij.modules.platform</depends>

    <extensions defaultExtensionNs="com.intellij">
        <codeInsight.lineMarkerProvider language="XML" implementationClass="com.ytanikin.datasetnavigator.provider.XmlLineMarkerProvider"/>
        <gotoDeclarationHandler order="first" implementation="com.ytanikin.datasetnavigator.goto.XmlGoToDeclarationHandler"/>
    </extensions>
</idea-plugin>
