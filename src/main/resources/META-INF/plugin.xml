<idea-plugin>
    <id>com.ytanikin.xmlwalker</id>
    <name>Xml Dataset Walker</name>
    <vendor>ytanikin</vendor>

    <!-- Product and plugin compatibility requirements -->
    <!-- https://plugins.jetbrains.com/docs/intellij/plugin-compatibility.html -->
    <depends>com.intellij.modules.platform</depends>

    <extensions defaultExtensionNs="com.intellij">
        <psi.referenceContributor implementation="com.ytanikin.datasetnavigator.contributor.XmlPsiReferenceContributor" language="XML" id="my" order="first"/>
        <codeInsight.lineMarkerProvider language="XML" implementationClass="com.ytanikin.datasetnavigator.provider.XmlLineMarkerProvider"/>
        <gotoTargetRendererProvider implementation="com.ytanikin.datasetnavigator.contributor.XmlGotoTargetRendererProvider" order="first"/>
    </extensions>
</idea-plugin>
